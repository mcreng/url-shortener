{
  "name": "url-shortener",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.8.2",
    "body-parser": "^1.18.3",
    "concurrently": "^3.5.0",
    "express": "^4.16.3",
    "knex": "^0.15.2",
    "mysql": "^2.16.0",
    "nodemon": "^1.18.3",
    "react": "^16.4.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.4.2",
    "react-google-login": "^3.2.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "client-start": "cd client && react-scripts start",
    "client-build": "cd client && react-scripts build",
    "client-test": "cd client && react-scripts test --env=jsdom",
    "client-eject": "cd client && react-scripts eject",
    "server-start": "cd server && nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server-start \" \"npm run client-start\"",
    "start": "node server/server.js",
    "heroku-postbuild": "cd client && npm run build"
  }
}
